{
  "name": "speech-to-text",
  "version": "1.0.0",
  "description": "basic speech-to-text app with speech synthesis",
  "main": "app.js",
  "private": true,
  "scripts": {
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=; NPM_CONFIG_PRODUCTION=false NODE_ENV=development npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;",
    "start": "node app.js",
    "build": "webpack -w --progress --color",
    "dev-start": "nodemon app.js",
    "build-css": "node-sass -w public/sass/layout.scss -o public/style",
    "test": "mocha ./tests/**/*.js --compilers js:babel-register",
    "build:v2": "cd v2 && yarn build"
  },
  "workspaces": ["v2"],
  "author": "Alejandro",
  "license": "ISC",
  "dependencies": {
    "annyang": "^2.6.0",
    "artyom.js": "^1.0.5",
    "axios": "^0.15.3",
    "body-parser": "^1.17.1",
    "bootstrap": "4.0.0-alpha.6",
    "express": "^4.17.3",
    "pg": "^7.0.2",
    "pg-hstore": "^2.3.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-icons": "^2.2.5",
    "react-tap-event-plugin": "^2.0.1",
    "sequelize": "^4.4.2",
    "webpack": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.4",
    "mocha": "^3.2.0",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.6",
    "sinon": "^1.17.7",
    "style-loader": "^0.18.2"
  }
}
